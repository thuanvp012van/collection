{
    "name": "penguin/collection",
    "description": "The Penguin Collection package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyễn Hoàng Thắng Thuận",
            "email": "thuanvp012van@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Penguin\\Component\\Collection\\": "src"
        },
        "files": [
            "./src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Penguin\\Collection\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "penguin/plug": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.3)."
    }
}
